              "get-started/why",
              "get-started/usage",
              "get-started/demo"
            ]
          },
          {
            "group": "API Reference",
            "pages": [
              "api/importer-props",
              "api/sheet-definition-props",
              "api/column-types",
              "api/common-column-props",
              "api/importer-state",
              "api/sheet-state",
              "api/validators",
              "api/transformers",
              "api/importer-validation-error"
            ]
          },
          {
            "group": "Configuration",
            "pages": ["configuration/peer-vs-bundled", "configuration/react"]
          },
          {
            "group": "Customization",
            "pages": [
              "customization/height-and-width",
              "customization/theme-styles"
            ]
          },
          {
            "group": "Development",
            "pages": [
              "development/setup",
              "development/no-build-example",
              "development/docs"
